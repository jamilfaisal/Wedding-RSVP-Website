{
  "name": "wedding-rsvp-website",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@11.3.0",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "next dev",
    "send-test-email": "tsx src/test-scripts/send_test_email.ts",
    "typecheck": "tsc --noEmit",
    "build": "npm run typecheck && next build && next-sitemap",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint && eslint --ext .js,.jsx,.ts,.tsx src",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,mjs,json,css,md,mdx,html}\"",
    "sitemap": "next-sitemap",
    "clean": "node -e \"const fs=require('fs');['node_modules','.next'].forEach(p=>fs.rmSync(p,{recursive:true,force:true}));console.log('Removed node_modules and .next');\"",
    "reset:deps": "node -e \"const fs=require('fs');['node_modules','.next','package-lock.json'].forEach(p=>fs.rmSync(p,{recursive:true,force:true}));console.log('Removed node_modules, .next, and lockfile');\""
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "0.6.1",
    "@headlessui/react": "2.2.7",
    "@heroicons/react": "2.2.0",
    "i18next": "25.4.1",
    "jose": "6.1.0",
    "jsonwebtoken": "9.0.2",
    "lucide-react": "0.539.0",
    "negotiator": "1.0.0",
    "next": "15.5.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-i18next": "15.7.2",
    "resend": "6.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@playwright/test": "1.54.2",
    "@tailwindcss/postcss": "4.1.12",
    "@types/dotenv": "6.1.1",
    "@types/jsonwebtoken": "9.0.10",
    "@types/negotiator": "0.6.4",
    "@types/node": "20.19.10",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "autoprefixer": "10.4.21",
    "dotenv": "17.2.1",
    "eslint": "9.33.0",
    "eslint-config-next": "15.4.6",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "next-sitemap": "4.2.3",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "4.1.12",
    "ts-node": "10.9.2",
    "tsx": "4.20.4",
    "typescript": "5.9.2"
  }
}
